{
    "name": "czim/laravel-datastore",
    "description": "Laravel data store framework.",
    "keywords": [
        "laravel",
        "api",
        "data abstraction",
        "data store"
    ],
    "homepage": "https://github.com/czim",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pxlwidgets.com",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "czim/laravel-dataobject": "^1.0|^2.0",
        "illuminate/support": "^5|^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0",
        "orchestra/database": "^4.0",
        "satooshi/php-coveralls": "^1.0",
        "czim/laravel-repository": "^2.0",
        "czim/laravel-filter": "^2.0",
        "doctrine/dbal": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Czim\\DataStore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\DataStore\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
