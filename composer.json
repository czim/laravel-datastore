{
    "name": "czim/laravel-datastore",
    "description": "Laravel data store framework.",
    "keywords": [
        "laravel",
        "api",
        "data abstraction",
        "data store"
    ],
    "homepage": "https://github.com/czim",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "czim/laravel-dataobject": "^1.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.5",
        "orchestra/database": "~3.5",
        "satooshi/php-coveralls": "^1.0",
        "czim/laravel-repository": "1.4.*",
        "doctrine/dbal": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Czim\\DataStore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\DataStore\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
